<div class="data-filler">
    <h1>Data from DB</h1>

    {{#if logs.length}}
    {{#each logs}}
        <div class="row">
            <div class="col s6 offset-s3">
                <div class="card">
                    <div class="card-content">
                        <p>Text: {{info}}</p>
                        <p>Number: {{num}}</p>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
    {{else}}
        <p>Empty Database</p>
    {{/if}}
</div>
